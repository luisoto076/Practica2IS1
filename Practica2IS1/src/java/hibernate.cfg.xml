    <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
    <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
    <property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/Practica</property>
    <property name="hibernate.connection.username">IS1</property>
    <property name="hibernate.connection.password">hola123</property>
    <mapping resource="Mapeo/Login.hbm.xml"/>
    <mapping resource="Mapeo/Usuario.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
